{
  "name": "my-diary",
  "version": "1.0.0",
  "description": "MyDiary is an online journal where users can pen down their thoughts and feelings.",
  "main": "app.js",
  "scripts": {
    "test": "npm run mig && nyc --reporter=text --reporter=lcov mocha --require babel-core/register ./server/tests/index.js  --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "babel-node app.js --exec  --presets es2015",
    "start:dev": "nodemon  app.js --exec babel-node --presets es2015",
    "mig": "babel-node ./server/migration/production.js"
  },
  "keywords": [
    "node",
    "express"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "coveralls": "^3.0.2",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "pg": "^7.4.3",
    "swagger-ui-express": "^3.0.10",
    "validator": "^10.4.0",
    "winston": "^3.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "mocha": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mrshinezee/My-Diary.git"
  },
